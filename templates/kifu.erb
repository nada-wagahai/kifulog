<html>
<head>
<meta charset="UTF-8">
<title><%= kifu.start_time %></title>
</head>
<body>
  <dl>
  <dt>開始日時</dt><dd><%= kifu.start_time %></dd>
  <dt>終了日時</dt><dd><%= kifu.end_time %></dd>
  <dt>手割合</dt><dd><%= kifu.handicap %></dd>
  <dt>棋戦</dt><dd><%= kifu.game_name %></dd>
  <dt>先手</dt><dd><%= kifu.first_players_name %></dd>
  <dt>後手</dt><dd><%= kifu.second_players_name %></dd>
  </dl>
  <table>
    <tr><th>手数</th><th>指手</th><th>消費時間</th></tr>
    <% kifu.steps.each do |step| %>
      <tr>
        <td><%= step.seq %></td>
        <td><%= step.move %></td>
        <td><%= step.elapsed_time %></td>
      </tr>
    <% end %>
  </table>
  <a href="../admin/download/<%= kifu.kifu_id %>">download</a>
  <a href="..">back</a>
</body>
</html>
